{
  "name": "trendify",
  "version": "1.0.0",
  "description": "A spotify user top lists generator.",
  "keywords": [
    "spotify",
    "track",
    "artists",
    "musics",
    "generator",
    "top"
  ],
  "private": true,
  "author": {
    "email": "asjuanguilherme@gmail.com",
    "name": "Juan Guilherme",
    "url": "https://juanguilher.me"
  },
  "scripts": {
    "postinstall": "yarn prepare",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "generate": "yarn plop --plopfile ./src/generators/plopfile.js",
    "format:fix": "prettier --write --ignore-unknown",
    "type:check": "tsc --noEmit --jsx react-jsx"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "html-to-image": "^1.11.11",
    "next": "13.4.19",
    "next-pwa": "^5.6.0",
    "nextjs-progressbar": "^0.0.16",
    "nookies": "^2.5.2",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.7",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jsdom": "^24.0.0",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^1.6.0"
  }
}
